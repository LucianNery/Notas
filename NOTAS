Algoritmo "Notas"

Var
  nota1, nota2, final : real


Inicio

      escreva ("Digite a primeira nota: ")
      leia (nota1)
      escreva ("Digite a segunda nota: ")
      leia (nota2)
      
      escreval ("NOTA FINAL = ", nota1 + nota2)
      
      se nota1 + nota2 <= 60.00 entao
         escreval ("REPROVADO!")
      senao
         escreval ("APROVADO")
      fimse
      
Fimalgoritmo
